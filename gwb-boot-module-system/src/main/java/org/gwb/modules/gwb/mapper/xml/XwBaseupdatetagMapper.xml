<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.gwb.modules.gwb.mapper.XwBaseupdatetagMapper">

    <select id="selectNewMaxUpdateTag" resultType="java.lang.Integer">
        SELECT MAX(p.updatetag) + 1 AS newMaxUpdateTag
        FROM ptype p
    </select>

</mapper>
